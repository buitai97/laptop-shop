{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "sourceMap": true,
        "skipLibCheck": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "src/*": [
                "./src/*"
            ],
            "controllers/*": [
                "./src/controllers/*"
            ],
            "models/*": [
                "./src/models/*"
            ],
            "routes/*": [
                "./src/routes/*"
            ],
            "services/*": [
                "./src/services/*"
            ],
            "views/*": [
                "./src/views/*"
            ],
            "config/*": [
                "./src/config/*"
            ],
        },
        
    }
}